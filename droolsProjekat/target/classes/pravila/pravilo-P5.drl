package pravila;

import model.Korisnik;
import model.Knjiga;
import java.util.Map;

global Korisnik ulogovaniKorisnik;
global Map<Knjiga, Double> ocenjeneKnjige;

rule "Izbacivanje vec lajkovanih knjiga"
salience 9
agenda-group "preporuka"
    when
    	$k: Knjiga() from ulogovaniKorisnik.lajkovaneKnjige
    then
        System.out.println("*** Knjiga " + $k + " je izbacena");
        ocenjeneKnjige.remove($k);
end
